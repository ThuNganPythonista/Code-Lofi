{
  "builds": [
    {
      "src": "djangLofi/wsgi.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "djangLofi/wsgi.py"
    }
  ],
    "installCommand": "python -m pip install --upgrade pip && pip install -r requirements.txt"
}
